<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.terrystest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message" >
        </activity>
        <activity
            android:name=".StartRound"
            android:label="@string/title_activity_start_round" >
        </activity>
        <activity
            android:name=".PlayerSelect"
            android:label="@string/title_activity_player_select" >
        </activity>
        <activity
            android:name=".NewPlayer"
            android:label="@string/title_activity_new_player" >
        </activity>
        <activity
            android:name=".CourseSelect"
            android:label="@string/title_activity_course_select" >
        </activity>
        <activity
            android:name=".PlayRound"
            android:label="@string/title_activity_play_round" >
        </activity>
        <activity
            android:name=".NewCourse"
            android:label="@string/title_activity_new_course" >
        </activity>
    </application>

</manifest>
